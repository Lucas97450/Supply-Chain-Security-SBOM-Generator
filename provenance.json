{
  "buildType": "https://github.com/sigstore/cosign",
  "builder": {
    "id": "github-actions"
  },
  "buildConfig": {
    "steps": [
      {
        "command": ["docker", "build", "-t", "supply-chain-security:latest", "."],
        "env": ["DOCKER_BUILDKIT=1"]
      },
      {
        "command": ["syft", ".", "-o", "json"],
        "env": []
      },
      {
        "command": ["grype", ".", "-o", "json"],
        "env": []
      },
      {
        "command": ["conftest", "test", "vulnerability-scan-project.json", "--policy", "security/policies/security.rego"],
        "env": []
      },
      {
        "command": ["cosign", "sign", "--key", "env://COSIGN_PRIVATE_KEY", "supply-chain-security:latest"],
        "env": ["COSIGN_PRIVATE_KEY"]
      },
      {
        "command": ["cosign", "attest", "--key", "env://COSIGN_PRIVATE_KEY", "--type", "sbom", "--predicate", "sbom-project.json", "supply-chain-security:latest"],
        "env": ["COSIGN_PRIVATE_KEY"]
      },
      {
        "command": ["cosign", "attest", "--key", "env://COSIGN_PRIVATE_KEY", "--type", "slsaprovenance", "--predicate", "provenance.json", "supply-chain-security:latest"],
        "env": ["COSIGN_PRIVATE_KEY"]
      }
    ]
  },
  "metadata": {
    "completeness": {
      "parameters": true,
      "environment": true,
      "materials": true
